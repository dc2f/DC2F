{
	'nodetype': 'classpath:com/dc2f/nodetype/renderable/template/HtmlTemplate',


	'source': '<div class="article">\n	$image\n	<div class="body">$body</div>\n</div>',
	'context': {
		'image': {
			'nodetype': 'classpath:com/dc2f/nodetype/ImageTagRenderer',
			'context': 'node',
			'ref': '.@image',
			'size': '500x300',
		},
		'body': {
			'nodetype': 'classpath:com/dc2f/nodetype/StringRenderer',
			'context': 'node',
			'ref': '.@body',
		},
	},
}
