{
	'nodetype': 'classpath:com/dc2f/nodetype/HtmlTemplate',


	'source': '
<div class="article">
	$image
	<div class="body">$body</div>
</div>
',
	'context': {
		'image': {
			'nodetype': 'classpath:com/dc2f/nodetype/ImageTagRenderer',
			'context': 'node',
			'ref': '.@image',
			'size': '500x300',
		},
		'body': {
			'nodetype': 'classpath:com/dc2f/nodetype/StringRenderer',
			'context': 'node',
			'ref': '.@body',
		},
	},
}
