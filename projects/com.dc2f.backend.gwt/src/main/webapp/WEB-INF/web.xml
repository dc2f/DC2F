<?xml version="1.0" encoding="UTF-8"?>
<web-app  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>DC2F.html</welcome-file>
  </welcome-file-list>
  <!-- Servlets -->
  
  <servlet>
    <servlet-name>DC2FNavigationServlet</servlet-name>
    <servlet-class>com.dc2f.backend.gwt.server.DC2FNavigationServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>DC2FNavigationServlet</servlet-name>
    <url-pattern>/dc2f/navigation</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DC2FContentServlet</servlet-name>
    <servlet-class>com.dc2f.backend.gwt.server.DC2FContentServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>DC2FContentServlet</servlet-name>
    <url-pattern>/dc2f/content</url-pattern>
  </servlet-mapping>
  

</web-app>
