<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.dc2f.backend.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/mywebapp/greet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>DC2FNavigationServlet</servlet-name>
    <servlet-class>com.dc2f.backend.server.DC2FNavigationServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>DC2FNavigationServlet</servlet-name>
    <url-pattern>/mywebapp/navigation</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>MyWebApp.html</welcome-file>
  </welcome-file-list>

</web-app>
